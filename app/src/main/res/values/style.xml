<?xml version="1.0" encoding="utf-8"?>
<resources>

    <style name="button_categories_style">
        <item name="android:text">@string/categories_arrow</item>
        <item name="android:textSize">15sp</item>
        <item name="android:layout_marginStart">15dp</item>
        <item name="android:textColor">@color/light_grey</item>

    </style>

    <style name="style_check_box">
        <item name="android:background">@drawable/shape_stroke_grey</item>
        <item name="android:textColor">@color/light_grey</item>
        <item name="android:textSize">10sp</item>
        <item name="android:layout_marginStart">6dp</item>
    </style>

    <style name="dialog_button">
        <item name="android:background">@drawable/shape_square_stroke_green</item>
        <item name="android:textAllCaps">false</item>
        <item name="android:textSize">@dimen/_20sp</item>
        <item name="android:gravity">left|center</item>
        <item name="android:textColor">@color/light_grey</item>
    </style>




</resources>